<h3>Artist Form</h3>

<%= form_for @artist do |f| %>
    <div>
        <%= f.label :name, "Aritist Name:" %>
        <%= f.text_field :name %>
    </div> <br>

   <div>
        <%= f.label :bio, "Artist Bio:" %>
        <%= f.text_area :bio %>     
   </div> <br>

    <%= f.submit "Create Artist" %>
<% end %>