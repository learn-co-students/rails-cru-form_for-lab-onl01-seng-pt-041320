<%= form_for @song do |f| %>
  <p><%= f.label "Artist ID" %></p>
  <p><%= f.text_field :artist_id %></p>
  <p><%= f.label "Genre ID" %></p>
  <p><%= f.text_field :genre_id %></p>
  <p><%= f.label "Song Name" %></p>
  <p><%= f.text_field :name %></p>
  <p><%= f.submit "Create Song" %></p>
  <% end %>